{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "noEmit": true,
        "noUncheckedIndexedAccess": true,
        "paths": {
            "https://cdnjs.cloudflare.com/ajax/libs/socket.io/*": [
                "./node_modules/socket.io-client/build/esm/index.d.ts"
            ]
        },
        "strict": true,
        "target": "ESNext"
    },
    "include": ["./*.js", "./*/**.js"]
}
